<!-- Page Heading -->
<div class="container-fluid">
    <h1 class="h3 mb-2 text-gray-800">User View</h1>
    
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Database User Views </h6>
        </div>
        <div class="card-body">

            <div class="table-responsive">
                <table class="table table-bordered table-striped" id="dataTable" width="100%" cellspacing="0">
                    <thead>
                        <tr class="table-success">
                            <th>Username</th>
                            <th>Password</th>
                            <th>Role</th>

                        </tr>
                    </thead>
                    <tfoot>
                        <tr class="table-success">
                            <th>Username</th>
                            <th>Password</th>
                            <th>Role</th>
                        </tr>
                    </tfoot>

                    <tbody>
                        <% data.forEach((value)=>{ %>
                            <tr>
                                <td>
                                    <%= value.username %>
                                </td>
                                <td>
                                    <%= value.password %>
                                </td>
                                <td>
                                    <%= value.role %>
                                </td>
                            </tr>
                            <% }) %>


                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>